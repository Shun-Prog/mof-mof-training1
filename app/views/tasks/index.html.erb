<h1>Tasks#index</h1>

<%= link_to '新規作成', new_task_path %>

<% @tasks.each do |task| %>

    <div class="task">
        <div class="task_column">
            <%= task.name %>
        </div>

        <div class="task_column" id="task_created_at">
            <%= task.created_at %>
        </div>
    </div>
    
    <%= link_to '詳細を見る', task_path(task) %>

<% end %>
